{
  "name": "CYOA-Remix",
  "private": true,
  "sideEffects": false,
  "license": "MIT",
  "epic-stack": true,
  "author": "Zach Justice",
  "type": "module",
  "imports": {
    "#*": "./*"
  },
  "scripts": {
    "build": "run-s build:*",
    "build:icons": "tsx ./other/build-icons.ts",
    "build:remix": "remix build --sourcemap",
    "build:server": "tsx ./other/build-server.ts",
    "predev": "npm run build:icons --silent",
    "dev": "remix dev -c \"node ./server/dev-server.js\" --manual",
    "prisma:studio": "prisma studio",
    "prettier:fix": "prettier --write .",
    "lint": "eslint .",
    "lint:fix": "eslint --fix --cache --cache-location ./node_modules/.cache/eslint .",
    "setup": "npm run build && prisma generate && prisma migrate deploy && prisma db seed && playwright install",
    "start": "cross-env NODE_ENV=production node .",
    "start:mocks": "cross-env NODE_ENV=production MOCKS=true tsx .",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "test:e2e": "npm run test:e2e:dev --silent",
    "test:e2e:dev": "playwright test --ui",
    "pretest:e2e:run": "npm run build",
    "test:e2e:run": "cross-env CI=true playwright test",
    "test:e2e:install": "npx playwright install --with-deps chromium",
    "typecheck": "tsc",
    "validate": "run-p \"test -- --run\" lint typecheck test:e2e:run",
    "prepare": "husky install"
  },
  "eslintIgnore": [
    "/node_modules",
    "/build",
    "/public/build",
    "/playwright-report",
    "/server-build"
  ],
  "dependencies": {
    "@conform-to/react": "^0.6.1",
    "@conform-to/zod": "^0.6.1",
    "@prisma/client": "^5.3.1",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-dialog": "^1.0.4",
    "@radix-ui/react-dropdown-menu": "^2.0.5",
    "@reach/dialog": "^0.18.0",
    "@reduxjs/toolkit": "1.9.0",
    "@remix-run/css-bundle": "^2.1.0",
    "@remix-run/express": "^2.1.0",
    "@remix-run/node": "^2.1.0",
    "@remix-run/react": "^2.1.0",
    "@remix-run/server-runtime": "^2.1.0",
    "@sentry/remix": "^7.57.0",
    "address": "^2.0.1",
    "bcryptjs": "^2.4.3",
    "better-sqlite3": "^8.3.0",
    "cachified": "^3.5.4",
    "chalk": "^5.3.0",
    "chokidar": "^3.5.3",
    "close-with-grace": "^1.2.0",
    "clsx": "^1.2.1",
    "compression": "^1.7.4",
    "cookie": "^0.5.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1",
    "downshift": "^7.6.0",
    "eslint-plugin-remix-react-routes": "^1.0.5",
    "execa": "^7.1.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.0.1",
    "flowbite": "^2.2.0",
    "flowbite-react": "^0.7.0",
    "fs-extra": "^11.1.1",
    "get-port": "^6.1.2",
    "helmet": "^7.0.0",
    "isbot": "^3.6.10",
    "litefs-js": "^1.1.2",
    "lru-cache": "^7.18.3",
    "morgan": "^1.10.0",
    "prisma": "^5.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.12.0",
    "react-redux": "^8.1.1",
    "remix-auth": "^3.5.1",
    "remix-auth-form": "^1.3.0",
    "remix-auth-github": "^1.6.0",
    "remix-auth-socials": "^2.0.5",
    "remix-utils": "^7.1.0",
    "spin-delay": "^1.2.0",
    "tiny-invariant": "^1.3.1",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@playwright/test": "^1.33.0",
    "@remix-run/dev": "^2.1.0",
    "@remix-run/eslint-config": "^2.1.0",
    "@remix-run/serve": "^2.1.0",
    "@remix-run/testing": "^2.1.0",
    "@testing-library/dom": "^9.2.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@total-typescript/ts-reset": "^0.4.2",
    "@types/bcryptjs": "^2.4.2",
