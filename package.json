    "@types/compression": "^1.7.2",
    "@types/cookie": "^0.5.0",
    "@types/eslint": "^8.37.0",
    "@types/express": "^4.17.17",
    "@types/fs-extra": "^11.0.1",
    "@types/glob": "^8.1.0",
    "@types/morgan": "^1.9.4",
    "@types/node": "^18.16.3",
